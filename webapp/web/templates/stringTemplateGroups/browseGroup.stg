group browseGroup : page;

/******************** MAIN ********************/
main() ::= <<

	<div id="content" class="siteMap">
	
		$if (message)$
			<p>$message$</p>
		$else$
			$classGroups: classGroup()$
		$endif$
	
	</div> <!-- content -->
>>

/******************** CLASSGROUP ********************/
classGroup() ::= <<

	<h2>$attr.publicName$</h2>
	<ul>
		$attr.vitroClassList: classList()$
	</ul>
>>